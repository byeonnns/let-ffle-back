<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mycompany.let_ffle.dao.TimeMissionDao">
   <select id="selectByRno" parameterType="int" resultType="TimeMission">
      select *
      from timemission
      where rno=#{rno}
   </select>
	<insert id="insertTimeMisson" parameterType="TimeMission">
	
		<selectKey keyProperty="rno" resultType="int" order="BEFORE">
		select seq_rno.currval from dual
	</selectKey>
		insert into timemission (rno,tstartedat, tfinishedat)
		values (#{rno}, #{tstartedat},
		#{tfinishedat})
	</insert>
</mapper>    